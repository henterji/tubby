{
  "name": "tubby-front",
  "version": "0.0.1",
  "description": "tubby front site",
  "scripts": {
    "clean": "rimraf -- node_modules doc coverage dist",
    "clean:install": "cnpm run clean && cnpm install",
    "clean:start": "cnpm run clean && cnpm start",
    "clean:dist": "rimraf -- dist",
    "build": "cnpm run clean:dist && webpack --config config/webpack.dev.js --progress --profile --bail",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --open --progress --profile --hot --watch --content-base src/",
    "server:prod": "node server.js",
    "lint": "cnpm run tslint \"src/**/*.ts\"",
    "build:docs": "cnpm run typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "start": "cnpm run server:dev",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && cnpm run protractor",
    "protractor:live": "protractor --elementExplorer",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "cnpm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
  "author": "Henter Liu <henterji@163.com>",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@angularclass/hmr": "~2.1.3",
    "@angularclass/hmr-loader": "~3.0.4",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "core-js": "^2.5.4",
    "ie-shim": "^0.1.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.8",
    "zone.js": "^0.8.26",
    "aphrodite": "^1.2.5",
    "lodash-es": "^4.17.8",
    "primeng": "^5.2.4",
    "echarts": "^4.0.4",
    "angular2-baidu-map": "^4.2.0",
    "moment": "^2.22.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^5.2.9",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "2.8.6",
    "@types/node": "^9.6.2",
    "@types/uglify-js": "^2.6.30",
    "@types/webpack": "^3.8.8",
    "ajv": "^6.4.0",
    "angular-router-loader": "^0.8.3",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.5.0",
    "codelyzer": "^4.2.1",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "~3.0.2",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy": "^1.16.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.1.0",
    "json-loader": "^0.5.7",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
