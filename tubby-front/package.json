    "string-replace-loader": "~2.1.1",
    "style-loader": "^0.20.3",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "^5.0.1",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "typedoc": "^0.11.1",
    "typescript": "~2.6.2",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0",
